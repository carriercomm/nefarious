  unet_cv_sys_nonblocking_posix=yes
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -fr conftest*
  unet_cv_sys_nonblocking_posix=no
fi
rm -fr conftest*
fi

fi

